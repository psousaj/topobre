{
    "extends": "@topobre/tsconfig/typeorm.json",
    "compilerOptions": {
        "paths": {
            "@topobre/typeorm/types": [
                "./types"
            ],
            "@topobre/typeorm": [
                "./entities"
            ],
            "@t3-oss/env-nextjs": [
                "../env/node_modules/@t3-oss/env-nextjs"
            ],
        },
        "composite": true,
        "declaration": true,
        "outDir": "./dist",
    },
    "include": [
        "."
    ],
    "exclude": [
        "dist",
        "build",
        "node_modules"
    ]
}