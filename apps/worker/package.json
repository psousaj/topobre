{
  "name": "@topobre/worker",
  "version": "0.0.0",
  "private": true,
  "main": "dist/bundle/index.js",
  "types": "dist/bundle/index.d.ts",
  "scripts": {
    "dev": "ts-node --project tsconfig.json -r tsconfig-paths/register src/index.ts",
    "build": "pnpm build:tsc && pnpm build:tsup",
    "build:tsc": "tsc -b",
    "build:tsup": "tsup",
    "start": "node dist/bundle/index.js"
  },
  "_moduleAliases": {
    "@topobre/finloader": "../finloader/dist",
    "@topobre/bullmq": "../../packages/bullmq/dist",
    "@topobre/env": "../../packages/env/dist",
    "@topobre/telemetry": "../../packages/telemetry/dist",
    "@topobre/typeorm": "../../packages/typeorm/dist",
    "@topobre/winston": "../../packages/winston/dist"
  },
  "dependencies": {
    "@topobre/finloader": "workspace:*",
    "@topobre/bullmq": "workspace:*",
    "@topobre/typeorm": "workspace:*",
    "@topobre/env": "workspace:*",
    "@topobre/telemetry": "workspace:*",
    "@topobre/winston": "workspace:*",
    "@topobre/gemini": "workspace:*"
  },
  "devDependencies": {
    "@topobre/tsconfig": "workspace:*"
  }
}